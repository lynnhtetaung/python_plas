	baseTxt = location.href.split("?")[1];
	$(function(){
		init();
	});
	</script> -->

	<!--[if lt IE 9]>
		<script type="text/javascript" src="demo/ie/htmlshiv.js"></script>
		<script type="text/javascript" src="demo/ie/respond.src.js"></script>
	<![endif]-->
	<script src="/javascripts/sha256.js" type="text/javascript"></script>
	<script type="text/javascript" src="/javascripts/scoring.js"></script>
	<script type="text/javascript" src="/javascripts/storage.js"></script>
	<script type="text/javascript" src="/javascripts/distinction.js"></script>
	<!-- <script src="javascript/index.js" type="text/javascript" charset="utf-8" ></script> -->
	<style>
	h2#statement:before{
		content:"Problem #";
	}
	code.fillinblank input{
		width:40px;
	}
	code{
		-moz-tab-size: 4 ;
		-o-tab-size: 4 ;
		tab-size: 4	 ;
	}
	</style>

</head>
<body>

<div class="bg-dark padding-y-7">
	<div class="container text-center">
		<h1>Offline JPLAS</h1>

		<!-- <a class="btn btn-success" onClick="toTop()">TOP</a> -->
		
            <a class="btn btn-success" a href="javascript:onclick=goBackToProblemList()" style="text-decoration: none">
                <!-- <h1 class="mt-3 mb-3 text-white">
                    <%= title %>
                </h1> -->
				TOP
            </a>

			<a class="btn btn-info" onclick="window.open('about:blank','_self').close()">END</a>
	</div>
	</div>

<div class="container">
	<div class="row">
		<div class="col md-3" style="display: none;">  
			<div class="sidemenu js-sidemenu">
				<h5>Sidemenu</h5>
				<div>
					<a href="#problems">Problems</a>
					<a href="#submission">Submission</a>
					<a href="#end">End</a>

				</div>
			</div>
		</div>
		
		<div class="col md-12 float-right jplas">   
			<%- question %>
			<button class="btn float-right" id="next" value="Next" onclick="onRouteChange('Next')" >Next</button>
			<button class="btn float-right margin-x-10" id="prev" value="Prev" onclick="onRouteChange('Prev')" >Prev</button>
		</div>
		<input type="hidden" id="totalProblems" value="<%= totalProblems %>" />
	</div>
</div>


<script type="text/javascript" src="/javascripts/programming.general.js"></script>
<script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
<script src="/javascripts/demo/js/jquery.sticky-kit.min.js"></script>
<script src="/javascripts/demo/google-code-prettify/prettify.js"></script>
<script src="/javascripts/demo/js/docs.js"></script>

</body>
</html>
